{
  "name": "my-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx dotenv -e .env.dev -- next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:init": "docker run --name local-postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_USER=postgres -e POSTGRES_DB=mydb -p 5433:5432 -v pgdata:/var/lib/postgresql/data -d ankane/pgvector",
    "db:start": "docker start local-postgres",
    "db:stop": "docker stop local-postgres",
    "db:status": "docker ps | grep local-postgres || echo 'Postgres is not running.'",
    "db:connect": "psql -h localhost -p 5433 -U postgres -d mydb",
    "db:push:dev": "npx dotenv -e .env.dev -- npx prisma db push && npx prisma generate",
    "db:push": "npx prisma db push && npx prisma generate"
  },
  "dependencies": {
